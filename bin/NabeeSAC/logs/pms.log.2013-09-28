        (
       SELECT MENU_ID, MAX(ACCESS_LEVEL) ACCESS_LEVEL
         FROM ACCESS_LIST
        WHERE ROLE_MEMBER_ID = 'kykim'
     GROUP BY MENU_ID
        ) ACCESS_LIST
       ON HIERARCHY.ID = ACCESS_LIST.MENU_ID
 WHERE ACCESS_LIST.MENU_ID IS NOT NULL OR LINK_VALUE IS NULL OR LINK_VALUE = ''  
