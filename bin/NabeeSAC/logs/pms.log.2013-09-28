*/

SELECT MENU.ID, 
       MENU.PARENT_ID, 
       HIERARCHY.LEVEL,
       MENU.IMAGE,
       MENU.NAME, 
       MENU.LINK_VALUE, 
       MENU_ORDER,
       IF(ACCESS_LEVEL IS NULL, 'AR001AR001', ACCESS_LEVEL) AS ACCESS_LEVEL
