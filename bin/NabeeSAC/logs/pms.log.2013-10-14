[2013-10-14 13:19:20,113] DEBUG [DatabaseSelectHandler.execute] EXECUTE ID : pms.project.select.project_r_30SELECT RPT.ID ID, 
       CASE RPT.REPORT_TYPE
       WHEN 'RPTYPRT001' THEN
            '일일보고'
       WHEN 'RPTYPRT002' THEN
            '주간보고'
       WHEN 'RPTYPRT003' THEN
            '월간보고'
       END AS RPT_TYPE_NAME,
       CASE RPT.REPORT_TYPE
       WHEN 'RPTYPRT001' THEN
            DATEDIFF(RPT.RGST_DTTM, DATE_FORMAT('20130812', '%Y%m%d')) + 1
       WHEN 'RPTYPRT002' THEN
            ROUND(DATEDIFF(DATE_SUB(RPT.RGST_DTTM, INTERVAL (DAYOFWEEK(RPT.RGST_DTTM) - 1) DAY), DATE_SUB(PJT.FROM_DATE, INTERVAL (DAYOFWEEK(PJT.FROM_DATE) - 1) DAY))/7 + 1)
       WHEN 'RPTYPRT003' THEN
            PERIOD_DIFF(DATE_FORMAT(RPT.RGST_DTTM, '%Y%m'), DATE_FORMAT(PJT.FROM_DATE, '%Y%m')) + 1
       END AS DIFF_COUNT,
       CASE RPT.REPORT_TYPE
       WHEN 'RPTYPRT001' THEN
            CONCAT(CONCAT(DATE_FORMAT(RPT.RGST_DTTM, '%Y년 %m월 %d일'), ' ~ '), DATE_FORMAT(RPT.RGST_DTTM, '%Y년 %m월 %d일'))
       WHEN 'RPTYPRT002' THEN
            IF(RPT.RGST_DTTM > PJT.FROM_DATE,
            CONCAT(CONCAT(DATE_FORMAT(DATE_SUB(RPT.RGST_DTTM, INTERVAL (DAYOFWEEK(RPT.RGST_DTTM) -1) DAY), '%Y년 %m월 %d일'), ' ~ '), DATE_FORMAT(DATE_ADD(RPT.RGST_DTTM, INTERVAL (7 - DAYOFWEEK(RPT.RGST_DTTM)) DAY), '%Y년 %m월 %d일')),
            CONCAT(CONCAT(DATE_FORMAT(DATE_SUB(PJT.FROM_DATE, INTERVAL (DAYOFWEEK(PJT.FROM_DATE) -1) DAY), '%Y년 %m월 %d일'), ' ~ '), DATE_FORMAT(DATE_ADD(RPT.RGST_DTTM, INTERVAL (7 - DAYOFWEEK(RPT.RGST_DTTM)) DAY), '%Y년 %m월 %d일')))
       WHEN 'RPTYPRT003' THEN
            IF(RPT.RGST_DTTM > PJT.FROM_DATE,
            CONCAT(CONCAT(DATE_FORMAT(RPT.RGST_DTTM, '%Y년 %m월 01일'), ' ~ '), DATE_FORMAT(LAST_DAY(RPT.RGST_DTTM), '%Y년 %m월 %d일')), 
            CONCAT(CONCAT(DATE_FORMAT(PJT.FROM_DATE, '%Y년 %m월 01일'), ' ~ '), DATE_FORMAT(LAST_DAY(PJT.FROM_DATE), '%Y년 %m월 %d일')))
       END AS PERIOD,
       DATE_FORMAT(RPT.RGST_DTTM, '%Y년 %m월 %d일') RGST_DTTM,
       RCV_ORG.NAME RECEIVER_ORGANIZATION,
       RCV_MBR.NAME RECEIVER,
       RPT_ORG.NAME MEMBER_ORGANIZATION,
       RPT_MBR.NAME MEMBER_NAME,
       RPT.RECEIVER RECEIVER_ID,
       RPT.RGST_MEMBER_ID MEMBER_ID
  FROM PROJECT PJT,
       REPORT RPT,
       MEMBER RCV_MBR, 
       MEMBER RPT_MBR, 
       PROJECT_MEMBER RCV_PMBR, 
       PROJECT_MEMBER RPT_PMBR, 
       ORGANIZATION RCV_ORG, 
       ORGANIZATION RPT_ORG
 WHERE RPT.RECEIVER = RCV_PMBR.MEMBER_ID
   AND RPT.PROJECT_ID = PJT.ID
   AND RPT.RGST_MEMBER_ID = RPT_PMBR.MEMBER_ID
   AND RCV_PMBR.MEMBER_ID = RCV_MBR.ID
   AND RPT_PMBR.MEMBER_ID = RPT_MBR.ID
   AND RCV_PMBR.ORGANIZATION_ID = RCV_ORG.ID
